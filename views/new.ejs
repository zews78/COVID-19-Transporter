  <div class="row">
    <form class="col s12" method="POST" action="/create">
      <div class="row">
        <div class="input-field col s6">
          <i class="material-icons prefix">account_circle</i>
          <input id="icon_prefix" type="text" class="validate" name="name">
          <label for="icon_prefix">Full Name</label>
        </div>
        <div class="input-field col s6">
          <i class="material-icons prefix">phone</i>
          <input id="icon_telephone" type="text" class="validate" name="telephone">
          <label for="icon_telephone">Telephone</label>
        </div>
        <div class="input-field col s6">
            <i class="material-icons prefix">add_location</i>
            <input id="icon_telephone" type="text" class="validate" name="address">
            <label for="icon_telephone">Address</label>
        </div>


        </div>
        <div class="row">
        <div class="input-field col s6">
            <i class="material-icons prefix">From</i>
            <input id="icon_telephone" type="text" class="validate" name="from">
            <label for="icon_telephone">Travel From</label>
        </div>
        <div class="input-field col s6">
            <i class="material-icons prefix">To</i>
            <input id="icon_telephone" type="text" class="validate" name="to">
            <label for="icon_telephone">Travel To</label>
        </div>
      </div>
      <div class="box">
          <p class="pi" style="text-align: center;font-family: timeburner;font-size: 2vw; color: white;">
              <a href="/new" class="btn-small grey">Reset</a>
              <input class="btn-small black" onclick="work()" type="submit" value="Submit">
          </p>

      </div>
    </form>
